{
  "Created": "Phunt_Vieg_",
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "white": "#f0f4ff",
    "pure_white": "#ffffff",
    "black": "#181825",
    "blue": "#89B4FA",
    "lavender": "#B4BEFE",
    "cyan": "#94E2D5",
    "green": "#A6E3A1",
    "purple": "#CBA6F7",
    "red": "#F38BA8",
    "pink": "#fad1ef",
    "yellow": "#F9E2AF",
    "orange": "#f9ab7b",
    "ctm_brown": "#2f3342",
    "ctm_blue": "#a4caff",
    "ctm_cyan": "#96facb",
    "ctm_orange": "#ff9d68"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:white",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "background": "p:ctm_orange",
          "foreground": "p:pure_white",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " ",

          "type": "os",
          "properties": {
            "display_distro_name": false,
            "override_icon": ""
          }
        },
        {
          "background": "p:ctm_cyan",
          "foreground": "p:black",
          "style": "diamond",
          "template": " {{ .UserName }} ",
          "trailing_diamond": "\ue0b0",
          "type": "session"
        },
        {
          "background": "p:ctm_blue",
          "foreground": "p:black",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": " \uf114  {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "p:pink",
          "foreground": "p:black",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "diamond",
          "template": " \ue725({{ .UpstreamIcon }} {{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        },
        {
          "background": "p:pink",
          "foreground": "p:black",
          "properties": {
            "time_format": "15:04"
          },
          "style": "diamond",
          "template": "{{ if ne (.Segments.Contains \"Git\") true }} \u2665  {{ .CurrentDate | date .Format }} {{ end }}",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:white",
          "style": "plain",
          "template": "\u2570\u2500<p:green>\u26a1</>",
          "type": "text"
        }
      ]
    }
  ],
  "transient_prompt": {
    "template": "<p:orange>\uf489 </><p:ctm_cyan>\uf0da</><p:ctm_cyan>\uf0da</> "
  },
  "final_space": true,
  "version": 7
}
